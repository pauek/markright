@parse-test(Hi)
  @input*
    hi
  @output*
    Markright
      Paragraph
        "hi"
@parse-test(A break)
  @input*
    hi
    ho
  @output*
    Markright
      Paragraph
        "hi"
        Break
        "ho"
@parse-test(Two paragraphs)
  @input*
    hi

    ho
  @output*
    Markright
      Paragraph
        "hi"
      Paragraph
        "ho"
@parse-test(Inline command)
  @input*
    a @b c
  @output*
    Markright
      Paragraph
        "a "
        InlineCmd(b)
        " c"
@parse-test(Block command)
  @input*
    @cmd(a, b, c)
      text content
  @output*
    Markright
      BlockCmd(cmd, ["a", "b", "c"])
        Paragraph
          "text content"
@parse-test(Empty Command)
  @input*
    Some text @ and @ two empty inline commands
  @output*
    Markright
      Paragraph
        "Some text "
        InlineCmd()
        " and "
        InlineCmd()
        " two empty inline commands"
@parse-test(Nested Delimiter)
  @input*
    @A{b @c{d} e}
  @output*
    Markright
      Paragraph
        InlineCmd(A)
          "b "
          InlineCmd(c)
            "d"
          " e"
@parse-test(Inline split into different lines)
  @input*
    Some text @cmd(1, 2, 3){The inside
    of this command is longer than usual
    bli bla blu} and some more
  @output*
    Markright
      Paragraph
        "Some text "
        InlineCmd(cmd, ["1", "2", "3"])
          "The inside"
          Break
          "of this command is longer than usual"
          Break
          "bli bla blu"
        " and some more"
@parse-test(Three inline commands)
  @input*
    @a @b @c
  @output*
    Markright
      Paragraph
        InlineCmd(a)
        " "
        InlineCmd(b)
        " "
        InlineCmd(c)
@parse-test(Indentation in inner text)
  @input*
    @raw*
      1st
        2nd
      3rd
  @output*
    Markright
      BlockCmd(raw*)
        "1st"
        "  2nd"
        "3rd"
@parse-test(Paragraph Command Paragraph)
  @input*
    hi

    @cmd

    ho
  @output*
    Markright
      Paragraph
        "hi"
      BlockCmd(cmd)
      Paragraph
        "ho"
@parse-test(Inline command in its own line)
  @input*
    some text
    @somecmd(a){inside text}
    some more text
  @output*
    Markright
      Paragraph
        "some text"
        Break
        InlineCmd(somecmd, ["a"])
          "inside text"
        Break
        "some more text"
@parse-test(More complex paragraph)
  @input*
    a @b c
    @d{e} @f(g)
  @output*
    Markright
      Paragraph
        "a "
        InlineCmd(b)
        " c"
        Break
        InlineCmd(d)
          "e"
        " "
        InlineCmd(f, ["g"])
@parse-test(More complex line)
  @input*
    Some line @strong[with @em{many @code<new>} commands] inside
  @output*
    Markright
      Paragraph
        "Some line "
        InlineCmd(strong)
          "with "
          InlineCmd(em)
            "many "
            InlineCmd(code)
              "new"
          " commands"
        " inside"
@parse-test(Empty lines)
  @input*

    @something

      Yay
  @output*
    Markright
      BlockCmd(something)
        Paragraph
          "Yay"
@parse-test(Nested Delimiter 2)
  @input*
    @a{b @c{d @e{f} g} h}
  @output*
    Markright
      Paragraph
        InlineCmd(a)
          "b "
          InlineCmd(c)
            "d "
            InlineCmd(e)
              "f"
            " g"
          " h"
@parse-test(Different delimiters 2)
  @input*
    @a<A>@b<<B>>@c<<<C>>>@d<<<<D>>>>
    @e[E]@f[[F]]@g[[[G]]]@h[[[[H]]]]
    @i{I}@j{{J}}@k{{{K}}}@l{{{{L}}}}
  @output*
    Markright
      Paragraph
        InlineCmd(a)
          "A"
        InlineCmd(b)
          "B"
        InlineCmd(c)
          "C"
        InlineCmd(d)
          "D"
        Break
        InlineCmd(e)
          "E"
        InlineCmd(f)
          "F"
        InlineCmd(g)
          "G"
        InlineCmd(h)
          "H"
        Break
        InlineCmd(i)
          "I"
        InlineCmd(j)
          "J"
        InlineCmd(k)
          "K"
        InlineCmd(l)
          "L"
@error-message-test(Wrong indentation)
  @input*
    @cmd1
        @inline1 a b c
      and more text
  @output*
    2:5: Parse Error: Wrong indentation
@error-message-test(Unfinished args)
  @input*
    @cmd(a, b, c
  @output*
    1:13: Parse Error: Found end of text while looking for ')'
@error-message-test(Block command in between paragraphs)
  @input*
    some text
    @somecmd(a)
      inside text
    some more text
  @output*
    2:1: Parse Error: Block command inside a paragraph
@parse-test(debug 1)
  @input*
    @B(a,b,c)
      line1 @
      line2
      line3
  @output*
    Markright
      BlockCmd(B, ["a", "b", "c"])
        Paragraph
          "line1 "
          InlineCmd()
          Break
          "line2"
          Break
          "line3"
@parse-test(debug 2)
  @input*
    bla bla
    @xxx
      1
    bli bli
  @output*
    Markright
      Paragraph
        "bla bla"
        Break
        InlineCmd(xxx)
        Break
        "1"
        Break
        "bli bli"
@parse-test(Break Command)
  @input*
    @block
      a b c
      d e f
    xxx
  @output*
    Markright
      BlockCmd(block)
        Paragraph
          "a b c"
          Break
          "d e f"
      Paragraph
        "xxx"
